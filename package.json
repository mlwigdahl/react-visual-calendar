{
  "name": "react-visual-calendar",
  "version": "0.0.3",
  "description": "React Visual Calendar is a responsive, mobile-first visual calendar web application designed to be used by children and by people with autism or other cognitive or developmental disabilities.",
  "engines": {
    "npm": ">=3"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "setup": "node tools/setup/setupMessage.js && npm install && node tools/setup/setup.js",
    "remove-demo": "babel-node tools/removeDemo.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist && npm run lint && npm run test",
    "build": "babel-node tools/build.js && npm run open:dist",
    "test": "jest --verbose",
    "test:cover": "npm run test -- --coverage ",
    "test:cover:travis": "babel-node node_modules/isparta/bin/isparta cover --root src --report lcovonly _mocha -- --require babel-polyfill ./tools/testSetup.js \"./{,!(node_modules)/**/}*.test.js\" && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "npm run test -- --watch",
    "open:cover": "npm run test:cover && open coverage/lcov-report/index.html"
  },
  "author": "Matt Wigdahl",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^3.3.6",
    "moment": "2.17.1",
    "object-assign": "4.1.0",
    "react": "^15.3.2",
    "react-addons-css-transition-group": "^15.3.2",
    "react-addons-shallow-compare": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^5.0.2",
    "react-router": "2.8.1",
    "react-router-redux": "^4.0.6",
    "redux": "3.6.0",
    "redux-saga": "^0.14.0",
    "reselect": "2.5.4"
  },
  "devDependencies": {
    "autoprefixer": "6.5.1",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.10",
    "babel-polyfill": "^6.16.0",
    "babel-preset-env": "^1.2.2",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-1": "^6.16.0",
    "babel-register": "^6.16.3",
    "browser-sync": "2.17.5",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "coveralls": "2.11.14",
    "cross-env": "3.1.3",
    "css-loader": "0.25.0",
    "enzyme": "2.7.1",
    "eslint": "3.8.1",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.4.1",
    "eslint-watch": "2.1.14",
    "extract-text-webpack-plugin": "^2.0.0-beta",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.24.0",
    "isparta": "4.0.0",
    "istanbul": "0.4.4",
    "jest": "19.0.2",
    "jsdom": "9.8.3",
    "jsdom-global": "2.1.0",
    "json-loader": "0.5.4",
    "mockdate": "1.0.4",
    "node-sass": "3.10.1",
    "npm-run-all": "3.1.1",
    "open": "0.0.5",
    "postcss-loader": "1.0.0",
    "prompt": "1.0.0",
    "react-addons-test-utils": "^15.3.2",
    "redux-immutable-state-invariant": "1.2.4",
    "redux-saga-test-plan": "^2.2.0",
    "replace": "0.3.0",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "^2.2.0",
    "webpack-bundle-analyzer": "1.5.2",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.0",
    "webpack-md5-hash": "0.0.5"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": ""
  }
}
